@model List<CRUDPractice.Models.Domain.Student>
@{
}
<h1> View Students</h1>
<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>RegistrationNumber</th>
			<th>PhoneNumber</th>
			<th>City</th>
			<th>University</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var student in Model)
		{
			<tr>
				<td>@student.Id</td>
				<td>@student.FirstName</td>
				<td>@student.LastName</td>
				<td>@student.RegistrationNumber</td>
				<td>@student.PhoneNumber</td>
				<td>@student.City</td>
				<td>@student.University</td>
				<td>
					<a href="Update/@student.Id">Update</a>
				</td>
			</tr>
		}
	</tbody>
</table>